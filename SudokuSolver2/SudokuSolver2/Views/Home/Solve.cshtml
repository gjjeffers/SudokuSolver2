@model SudokuSolver2.Models.Puzzle
        @{int index = 0; }
        <table style="border: 3px solid black;">
        @for(var x=1; x < 10; x++)
        {
            <tr>
                @for (var y = 1; y < 10; y++)
                {
                    <td>@Html.Label(Model.Grid[index].Value.ToString())</td>
                    index++;
                }
            </tr>
        }
        </table>
